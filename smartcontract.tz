{ parameter (or (unit %empty) (mutez %make_transaction)) ;
  storage nat ;
  code { UNPAIR ;
         IF_LEFT
           { DROP ; NIL operation ; PAIR }
           { PUSH mutez 10000000 ;
             SWAP ;
             COMPARE ;
             EQ ;
             IF { PUSH nat 1 ; ADD ; NIL operation ; PAIR }
                { DROP ; PUSH string "Incorrect amount, Please transfer 10 tez" ; FAILWITH } } } }

